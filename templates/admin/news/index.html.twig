{% extends 'base.html.twig' %}

{% block title %}News{% endblock %}

{% block content %}
	<a href="{{ path( 'app_admin_news_add' ) }}" title="Add News">Add News</a><br />
	<h1>News</h1>

	{% for entry in news %}
		<div>
			<p>
				<a href="{{ path( 'app_admin_news_edit', { id: entry.id }) }}" title="{{ entry.title }}">{{ entry.title }}</a>
			</p>
		</div>
	{% endfor %}
{% endblock %}
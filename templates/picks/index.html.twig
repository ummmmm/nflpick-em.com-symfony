{% extends 'base.html.twig' %}

{% block title %}Picks{% endblock %}

{% block content %}
    <h1>Weeks</h1>

    {% for week in weeks %}
        <p><a href="{{ path( 'app_picks_week', { 'id': week.id }) }}">Week {{ week.id }}</a>
            {% if week.locked %}
            - <b>Locked</b>
            {% endif %}
        </p>
    {% endfor %}
{% endblock %}
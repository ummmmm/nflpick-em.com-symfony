{% extends 'base.html.twig' %}

{% block title %}Leaderboard{% endblock %}

{% block content %}
	<h1>Leaderboard</h1>

	{% for user in users %}
		<p>{{ user.currentPlace }}<sup>{{ user.getOrdinal }}</sup> {{ user.name }} - {{ user.wins }} Wins - {{ user.losses }} Losses</p>
	{% endfor %}
{% endblock %}
